{
  "name": "react-material-ui-carousel-alt",
  "version": "0.0.2",
  "description": "React Material UI Carousel from Learus with altered peerDependencies for @material-ui/*@4 ",
  "keywords": [
    "react",
    "material",
    "material ui",
    "carousel",
    "gallery"
  ],
  "homepage": "https://learus.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/Learus/react-material-ui-carousel"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "npx rimraf dist && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "lint": "eslint 'src' --fix"
  },
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "framer-motion": "6.5.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.52",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.55.1",
    "eslint-plugin-react": "^7.31.11",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0 || <5.0.0",
    "@material-ui/icons": "^4.11.0 || <5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependenciesMeta": {
    "@material-ui/core": {
      "optional": false
    },
    "@material-ui/icons": {
      "optional": false
    },
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  }
}
